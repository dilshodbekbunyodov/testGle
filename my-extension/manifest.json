{
  "manifest_version": 3,
  "name": "My Vue Chrome Extension",
  "version": "1.0",
  "description": "A Chrome extension built with Vue 3 and Vite",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["scripting"],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
